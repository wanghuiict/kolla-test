# default settings
[ -z "$TIANNI_SRC" ] && export TIANNI_SRC=/dev/sr0
[ -z "$TIANNI_DIR" ] && export TIANNI_DIR=/media/cdrom

[ -z "$WORKDIR" ] && export WORKDIR="/home/kolla"
[ -z "$VENVDIR" ] && export VENVDIR="usr"
[ -z "$KOLLADIR" ] && export KOLLADIR="${WORKDIR}/${VENVDIR}/share/kolla-ansible"
[ -z "$ETCKOLLA" ] && export ETCKOLLA="/etc/kolla"
[ -z "$INVENTORY" ] && export INVENTORY="multinode"

echo "TIANNI_SRC: $TIANNI_SRC"
echo "TIANNI_DIR: $TIANNI_DIR"
echo "WORKDIR: $WORKDIR"
echo "VENVDIR: $VENVDIR"
echo "KOLLADIR: $KOLLADIR"
echo "ETCKOLLA: $ETCKOLLA"
echo "INVENTORY: $INVENTORY"

#read -p "press any key to continue..." none
sleep 2

